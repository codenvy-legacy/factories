{
  "v": "2.1",
  "project": {
    "name": "console",
    "description": "A simple JAR project based on Maven.",
    "visibility": "public",
    "builders": {
      "default": "maven"
    },
    "runners": {
      "default": "system:/java/standalone/simple/cli"
    },
    "type": "maven",
    "attributes": {
      "language": [
        "java"
      ],
      "contribute": [
        "github"
      ]
    }
  },
  "source": {
    "project": {
      "location": "https://github.com/Serli/codenvy-contribution-sample",
      "type": "git",
      "parameters": {
        "branch": "master",
        "keepVcs": "true"
      }
    }
  },
  "ide": {
    "onProjectOpened": {
      "actions": [
        {
          "id": "openFile",
          "properties": {
            "file": "README.md"
          }
        }
      ]
    }
  },
  "creator": {
    "name": "Sun TAN",
    "email": "sun.tan@serli.com"
  }
}
